<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HotelTextView">
        <attr name="hotel_text_drawable_src" format="reference" />
        <attr name="hotel_text_drawable_direction">
            <enum name="hotel_left" value="0" />
            <enum name="hotel_top" value="1" />
            <enum name="hotel_right" value="2" />
            <enum name="hotel_bottom" value="3" />
        </attr>
        <attr name="hotel_text_drawable_width" format="dimension" />
        <attr name="hotel_text_drawable_height" format="dimension" />
        <attr name="hotel_text_drawable_padding" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StockTitleView">
        <attr name="stock_title_text_msg" format="string|reference" />
        <attr name="stock_title_text_style" format="string|reference" />
    </declare-styleable>

</resources>