<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.xt.lxl.stock.widget.view.StockTitleView
        android:id="@+id/stock_title_view"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_alignParentTop="true"
        app:stock_title_text_msg="关于我们" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:gravity="center"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/stock_version_icon"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:background="@drawable/stock_search" />

        <TextView
            android:id="@+id/stock_version_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="版本号：" />

    </LinearLayout>


</RelativeLayout>